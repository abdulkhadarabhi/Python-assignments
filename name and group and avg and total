sno=int(input('enter the student rollno:'))
sname=input('enter the student name:')
gender=input('enter the student gender:')
group=input('enter student group:')
address=input('enter the student address:')
phno=int(input('enter the student phone number:'))
print('student rollno','is',sno)
print('student name','is',sname)
print('student gender','is',gender)
print('student group','is',group)
print('student address','is',address)
print('student phone number','is',phno)
